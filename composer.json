{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"require": {
		"laravel/framework": "4.2.*",
		 "omnipay/common": "~2.3.0",
		"ignited/laravel-omnipay": "1.*",
		"omnipay/paypal": "*",
		"aws/aws-sdk-php-laravel": "1.*",
		"indatus/dispatcher": "~1.4",
        "php-ffmpeg/php-ffmpeg": "~0.5",
        "sybio/gif-frame-extractor": "dev-master",
        "abraham/twitteroauth": "0.5.2",
		"guzzlehttp/guzzle": "~4.0",
		"jenssegers/imagehash": "^0.2.3"
	},
	"autoload": {
		"classmap": [
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php",
            "app/spacexstats",
			"app/config/credentials.php",
			"app/macros.php"
		],
		"psr-0": {
			"Ignited\\LaravelOmnipay": "src/"
		}
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable"
}
